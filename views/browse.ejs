<!DOCTYPE html>
<html lang="en">

	<head>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
		<link href="/css/main.css" rel="stylesheet">
		<link href="/css/browse.css" rel="stylesheet">

	</head>

	<body>
		
		<iframe id="downloadframe" style="display:none"></iframe>

		<div class="col-md-12">

			<% include header %>
			
			<div class = "row content-row">
				
				<div class="col-lg-1 col-centered content-col" id="my_data">
					
					<button type="button" class="btn btn-default" id="btn_back">
						<span class="glyphicon glyphicon-chevron-left"></span> Back to My Data
					</button>
					
					<div class="panel panel-default panel-regiondata">
						
						<div class="panel-heading heading-regiondata">My Data</div>
						
						<table class="table">
							
							<thead id="content_head">

							</thead>
							
							<tbody id="content_data">
								
							</tbody>
							
						</table>
						
					</div>
					<button class="btn btn-success" id="btn_save">
						Save
					</button>
					
				</div>
				
			</div>
			
			<div class="row content-row">

				<div class="col-lg-1 col-centered content-col" id="footer_bot">

					<div class="footer" id="foot_bot">
						
					</div>

				</div>

			</div>
			
		</div>
			
		<!-- <div class='scrolldown'>Scroll Down for more entries.</div> -->

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
		<script src="bootstrap/js/bootstrap.min.js"></script>
		<script src="js/utils.js"></script>
		<script src="js/browse.js"></script>

	</body>
</html>