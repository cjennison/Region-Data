<!DOCTYPE html>
<html lang="en">

	<head>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
		<link href="/css/main.css" rel="stylesheet">
		<link href="/css/settings.css" rel="stylesheet">

	</head>

	<body>
		
		<iframe id="downloadframe" style="display:none"></iframe>

		<div class="col-md-12">

			<div class="row content-row">

				<div class="col-lg-1 col-centered content-col" id="navigation_top">

					<div class="navbar navbar-default navbar-fixed-top" role="navigation" id="nav_top">
						<div class="container">
							<div class="navbar-header">
								<a class="navbar-brand" href="#"><img src="img/logo_regiondata_white.png" id="logo"></a>
							</div>
							<div class="navbar-collapse collapse">
								<ul class="nav navbar-nav">
									<li class="btn-nav-top-2" id="button_mydata">
										<a href="/main">My Data</a>
									</li>
									<li class="btn-nav-top" id="button_explore">
										<a href="/explore">Explore</a>
									</li>
									<li>
										<a href="#contact"></a>
									</li>
									
								</ul>
								
							</div>
							
						</div>
						
					</div>

				</div>
				
				<div class="row content-row">
					
					<div class="col-log-1 col-centered content-col">
						
						<form class="form-horizontal" role="form">
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Name</label>
								<div class="col-sm-10">
									<input class="form-control" id="field_name">
								</div>
							</div>
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Subject</label>
								<div class="col-sm-10">
									<input class="form-control" id="field_subject">
								</div>
							</div>
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Description</label>
								<div class="col-sm-10">
									<textarea rows="5" type="password" class="form-control" id="field_description"></textarea>
								</div>
							</div>
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Size</label>
								<div class="col-sm-10">
									<p class="form-control-static" id="field_size">Size</p>
								</div>
							</div>
							
							<hr class="divider-horz-margin">
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Share Publicly</label>
								<div class="col-sm-10">
									<div class="btn-group btn-toggle">
										<button type="button" class="btn btn-default">
											ON
										</button>
										<button type="button" class="btn btn-primary active">
											OFF
										</button>
									</div>
								</div>
							</div>
							
							<div class="form-group">
								<label class="col-sm-2 control-label">Allow Forking</label>
								<div class="col-sm-10">
									<div class="btn-group btn-toggle">
										<button type="button" class="btn btn-default">
											ON
										</button>
										<button type="button" class="btn btn-primary active">
											OFF
										</button>
									</div>
								</div>
							</div>
							
							<hr class="divider-horz-margin">
							
							<button type="button" class="btn btn-lg btn-regiondata" id="btn_save">Save</button>
							
						</form>
						
					</div>
					
				</div>

			</div>
			
		</div>
			
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
		<script src="bootstrap/js/bootstrap.min.js"></script>
		<script src="js/utils.js"></script>
		<script src="js/settings.js"></script>

	</body>
</html>