<!DOCTYPE html>
<html lang="en">

	<head>

		<title>RegionData</title>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
		<link href="/css/main.css" rel="stylesheet">
		<link href="/css/model.css" rel="stylesheet">

	</head>

	<body onload='init()'>
		
		<iframe id="downloadframe" style="display:none"></iframe>

		<div class="col-md-12">

			<!-- TOP BAR -->
			<div class="row content-row">

				<div class="col-lg-1 col-centered content-col" id="navigation_top">

					<div class="navbar navbar-default navbar-fixed-top" role="navigation" id="nav_top">
						<div class="container">
							<div class="navbar-header">
								<a class="navbar-brand" href="#"><img src="img/logo_regiondata_white.png" id="logo"></a>
							</div>
							<div class="navbar-collapse collapse">
								<ul class="nav navbar-nav">
									<li class="btn-nav-top-2" id="button_mydata">
										<a href="#">My Data</a>
									</li>
									<li class="btn-nav-top" id="button_explore">
										<a href="#">Explore</a>
									</li>
									<li>
										<a href="#contact"></a>
									</li>
								</ul>
							</div>
						</div>
					</div>

				</div>

			</div>
			
			
			<div class="row content-row">
				
				<!-- SnapShot -->
				<div class="col-lg-4 col-centered content-col" id="db-tomodel">
					<h1>Modeling: </h1><h3>Loading ...</h3> <hr />
					<div class="panel panel-default">
						<div class="panel-heading heading-regiondata">Snapshot</div>
						<table class="table">
							<thead>
								<tr></tr>
							</thead>
						
							<tbody id="content_data">
								
							</tbody>
						</table>
						
						
					</div>
				</div>
				
				<!-- My Models -->
				<div class="col-lg-4 col-centered content-col" id="db-tomodel">
					<h1>Models</h1>
					<div class="panel panel-default">
						<div class="panel-heading heading-regiondata"></div>
						<table class="table">
							<thead>
								<tr></tr>
							</thead>
						
							<tbody id="content_data">
								<tr><td>
								<h3>Mean</h3> <button class='btn btn-primary usemodelbtn' onclick="useModel('public/mean', this)">Use Model</button>
								<p>
									Returns the mean of a given time interval
								</p>
								
								<div class='modelinfo'></div>
								
								</td></tr>
							</tbody>
						</table>
						
						
					</div>
				</div>
			</div>
			
			
			
		<!-- END COL -->
		</div>
		<script src='http://requirejs.org/docs/release/2.1.14/minified/require.js'></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
		<script src="bootstrap/js/bootstrap.min.js"></script>
		<script src="js/utils.js"></script>
		<script src="js/model.js"></script>
	</body>
</html>