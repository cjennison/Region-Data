<!DOCTYPE html>
<html lang="en">

	<head>

		<title>RegionData</title>

		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
		<link href="/css/main.css" rel="stylesheet">
		<link href="/css/model.css" rel="stylesheet">

	</head>

	<body onload='init()'>
		
		<iframe id="downloadframe" style="display:none"></iframe>

		<div class="col-md-12">

			<!-- TOP BAR -->
			<% include header %>
			
			
			<div class="row content-row">
				
				<!-- SnapShot -->
				<div class="col-lg-4 col-centered content-col" id="db-tomodel">
					<h1>Modeling: </h1><h3>Loading ...</h3> <hr />
					<div class="panel panel-default">
						<div class="panel-heading heading-regiondata">Snapshot</div>
						<table class="table">
							<thead>
								<tr></tr>
							</thead>
						
							<tbody id="content_data">
								
							</tbody>
						</table>
						
						
					</div>
				</div>
				
				<!-- My Models -->
				<div class="col-lg-4 col-centered content-col" id="db-tomodel">
					<h1>Models</h1>
					<div class="panel panel-default">
						<div class="panel-heading heading-regiondata"></div>
						<table class="table">
							<thead>
								<tr></tr>
							</thead>
						
							<tbody id="content_data">
								<tr><td>
								<h3>Mean</h3> <button class='btn btn-primary usemodelbtn' onclick="useModel('public/mean', this)">Use Model</button>
								<p>
									Returns the mean of a given time interval
								</p>
								
								<div class='modelinfo'></div>
								
								</td></tr>
							</tbody>
						</table>
						
						
					</div>
				</div>
			</div>
			
			
			
		<!-- END COL -->
		</div>
		<script src='http://requirejs.org/docs/release/2.1.14/minified/require.js'></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
		<script src="bootstrap/js/bootstrap.min.js"></script>
		<script src="js/utils.js"></script>
		<script src="js/model.js"></script>
	</body>
</html>